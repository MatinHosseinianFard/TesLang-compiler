proc add
    mov r2, 0
    add r3, r0, r1
    mov r2, r3
    mov r0, r2
    ret

proc scan
    call iget, r0
    ret

proc print
    call iput, r0
    ret

proc main
    mov r0, 0
    mov r1, 0
    call scan, r2
    mov r0, r2
    call scan, r3
    mov r1, r3
    mov r4, r0
    call add, r4, r1
    mov r5, r4
    call print, r5
    mov r6, 0
    mov r0, r6
    ret


